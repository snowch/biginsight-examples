<Beans>
    <Bean Class="net.sourceforge.squirrel_sql.plugins.sqlscript.prefs.SQLScriptPreferenceBean">
        <clientName>SQuirreL SQL Client/SQL Scripts Plugin</clientName>
        <clientVersion>3.7.1/1.3</clientVersion>
        <deleteAction>0</deleteAction>
        <deleteRefAction>false</deleteRefAction>
        <escapeNewLine>true</escapeNewLine>
        <escapeNewLineString>\n</escapeNewLineString>
        <qualifyTableNames>true</qualifyTableNames>
        <updateAction>0</updateAction>
        <updateRefAction>false</updateRefAction>
        <useDoubleQuotes>true</useDoubleQuotes>
    </Bean>
</Beans>
